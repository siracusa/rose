#!/usr/bin/perl

use lib 'lib';

use FindBin qw($Bin);

require 't/test-lib.pl';
  
use Storable;

my $frozen_file = "$Bin/frozen";
open(my $fh, $frozen_file) or die "Could not open $frozen_file - $!";
my $frozen = do { local $/; <$fh> };
close($fh);

my $thawed = Storable::thaw($frozen);

$thawed->dbh->do('DROP TABLE rose_db_storable_test');

print "dropped\n";
